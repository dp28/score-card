{
  "name": "score-card-server",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.mjs",
  "scripts": {
    "start": "BABEL_DISABLE_CACHE=1 nodemon --exec npx babel-node --ignore '/node_modules/(?!score-card-domain)' -- src/server.mjs",
    "test": "jest --watchAll --notify",
    "schema": "BABEL_DISABLE_CACHE=1 npx babel-node src/alexa/generateSchema.js | tail -n+2 > alexa_skill_schema.json && echo 'Wrote to alexa_skill_schema.json'"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/dp28/score-card"
  },
  "author": "Daniel Patterson <dan24patt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "alexa-app": "^4.2.2",
    "express": "^4.16.3",
    "express-ws": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.2.0",
    "nodemon": "^1.17.5",
    "regenerator-runtime": "^0.12.0"
  },
  "jest": {
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "testMatch": [
      "**/*.spec.mjs"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "mjs",
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
